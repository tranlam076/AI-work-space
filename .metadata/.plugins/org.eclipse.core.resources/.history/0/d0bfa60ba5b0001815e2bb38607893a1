package connectToServer;

import java.net.Socket;

public class Connect {

	public static void main(String[] args) {
		try {
			Socket soc = new Socket("google.com", 80);
			
			System.out.println(soc.getInetAddress());
			
			System.out.println("success");
			soc.close();
		} catch (Exception e) {
			
		}
	}

}
